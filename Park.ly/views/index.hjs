<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

    <ul>
      hi


      {{! user name }}
      {{#users}}
        <li> Name: {{first_name}}
          {{! spots }}
          <ol> Available Spots
            {{#Spots}}
              <li>{{address}}</li>
            {{/Spots}}
          </ol>
          {{! revervations }}
          <ol> Upcoming Reservations
            {{#Reservations}}
              <li>{{start_date_time}}</li>
            {{/Reservations}}
          </ol>
        </li>
      {{/users}}
